<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <script type="systemjs-importmap" crossorigin="anonymous">
      <%- JSON.stringify(systemJsImportMapConfig) %>
    </script>
  </head>
  <body>
    <div id="root"></div>
    <script>
      function onSystemReady() {
        System.import('http://localhost:3001/main.js');
      }
    </script>
    <script src="https://cdn.jsdelivr.net/npm/systemjs@6.14.0/dist/system.js" onload=onSystemReady()></script>
    <meta name="importmap-type" content="systemjs-importmap" />
  </body>
</html>