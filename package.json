{
  "name": "@devhub/starter-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "npx rimraf dist",
    "dev": "run-p server:dev client:dev",
    "client:dev": "npx webpack --config ./bundle/client/dev.js",
    "client:build": "cross-env NODE_ENV=production npx webpack --config ./bundle/client/build.js",
    "client-types": "npx tsc --project ./tsconfig-shell.types.json",
    "client-types:dev": "tsc-watch --project ./tsconfig-shell.types.json --onSuccess 'npx yalc push'",
    "client-types:extract": "yarn client-types && npx api-extractor run --local",
    "client-types:check": "yarn client-types && npx api-extractor run",
    "server:dev": "ts-node-dev --project tsconfig.server.json --respawn --ignore-watch node_modules src/server",
    "server:build": "tsc --project tsconfig.server.json",
    "build": "yarn clean && yarn server:build && yarn client:build",
    "test": "tsc && prettier --check . && eslint . && stylelint \"**/*.css\"",
    "test:playwright": "playwright test",
    "start": "node dist/server.js"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.200",
    "@types/node": "22.9.0",
    "@types/react": "^18.2.62",
    "@types/react-dom": "^18.2.19",
    "@types/react-refresh": "^0",
    "@types/react-router-dom": "^5.3.1",
    "@types/serve-favicon": "^2.5.7",
    "@types/systemjs": "^6.15.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-loader": "^9.2.1",
    "chalk": "^5.4.1",
    "concurrently": "^6.3.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "http-proxy-middleware": "^2.0.1",
    "lint-staged": "^13.3.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.51.1",
    "prettier": "^3.5.3",
    "progress-bar-webpack-plugin": "^2.1.0",
    "style-loader": "^4.0.0",
    "stylelint": "^14.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^23.0.0",
    "thread-loader": "^4.0.4",
    "ts-loader": "^9.5.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.8.2",
    "vite": "^6.2.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@devhub/1fe-server": "1.0.10",
    "@devhub/1fe-shell": "1.0.10",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "11.14.0",
    "core-js": "^3.40.0",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-validator": "^7.0.1",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-refresh": "^0.16.0",
    "react-router-dom": "6.28.1",
    "serve-favicon": "^2.5.0",
    "systemjs": "^6.15.1"
